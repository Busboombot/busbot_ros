<launch>
  <node name="joystick" pkg="joy" type="joy_node" />
  <param name="/joystick/dev" value="/dev/input/js0" />
  <!-- <param name="/joystick/autorepeat_rate" value="10" /> -->
  <param name="/joystick/coalesce_interval" value="0.05" />

  <node name="stepper" pkg="stepper" type="stepper_node" />

  <rosparam command="load" file="$(find stepper)/config.yaml" />
  
</launch>